{
  "name": "unfun-ts",
  "version": "1.0.0",
  "description": "Practical helpers to unfunctionalize fp-ts types",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc -b tsconfig.build.json",
    "typecheck": "tsc -b",
    "clean": "rimraf lib/*",
    "prebuild": "yarn clean",
    "format": "prettier --write \"{tests,mocks,src}/**/*.ts\"",
    "format:check": "prettier --check \"{tests,mocks,src}/**/*.ts\"",
    "docs": "docs-ts"
  },
  "keywords": [
    "fp-ts",
    "functional",
    "typescript"
  ],
  "author": "Jeff Dolle <jeff@jeffdolle.com>",
  "homepage": "https://github.com/jdolle/unfun-ts",
  "license": "MIT",
  "peerDependencies": {
    "fp-ts": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "25.1.2",
    "docs-ts": "^0.3.4",
    "fp-ts": "^2.0.0",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "ts-jest": "25.1.0",
    "typescript": "3.7.5"
  }
}
